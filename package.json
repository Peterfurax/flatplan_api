{
  "name": "flatplan_api",
  "version": "1.0.3",
  "description": "dev in progress",
  "main": "src/server.js",
  "scripts": {
    "prebabels": "echo ------------------------------ BABEL INIT ------------------------------",
    "babels": "npm-run-all babel:*",
    "babel:server": "babel --presets es2015 src/server.js  --out-file build/server.js",
    "babel:cluster": "babel --presets es2015 src/cluster.js  --out-file build/cluster.js",
    "babel:lib": "babel --presets es2015 src/provider/data.js  --out-file build/provider/data.js",
    "postbabels": "echo ------------------------------ BABEL FINISH ------------------------------",
    "start": "node build/server.js",
    "start:dev": "node src/server.js",
    "cluster": "node build/cluster.js",
    "cluster:dev": "node src/cluster.js",
    "precleans": "echo ------------------------------ CLEAN INIT ------------------------------",
    "cleans": "npm-run-all clean:* ",
    "clean:coverage": "rimraf ./coverage",
    "clean:docs": "rimraf ./docs",
    "postcleans": "echo ------------------------------ CLEAN FINISH ------------------------------",
    "watch": "grunt watch",
    "watch:prod": "nodenon build/server.js",
    "watch:dev": "nodenon src/server.js",
    "prelints": "echo ------------------------------ LINT INIT ------------------------------",
    "lints": "npm-run-all lint:*",
    "lint:server": "jshint src/server.js",
    "lint:cluster": "jshint src/cluster.js",
    "lint:lib": "jshint src/lib/lib.js",
    "postlints": "echo ------------------------------ LINT FINISH ------------------------------",
    "predocs": "echo ------------------------------ DOC INIT ------------------------------",
    "docs": "npm-run-all doc:* ",
    "doc:test": "docco test/test.js --layout linear",
    "doc:testGetErr": "docco test/provider/getTestErr.js --layout linear ",
    "doc:testGet": "docco test/provider/getTest.js --layout linear",
    "doc:server": "docco src/server.js --layout linear",
    "doc:cluster": "docco src/cluster.js --layout linear",
    "doc:lib": "docco src/lib/lib.js --layout linear",
    "postdocs": "echo ------------------------------ DOC FINISH ------------------------------",
    "preunitTest": "echo ------------------------------ UNIT TEST INIT ------------------------------",
    "unitTest": "nyc --reporter=html --reporter=text mocha",
    "postunitTest": "echo ------------------------------ UNIT TEST FINISH ------------------------------",
    "pretest": "echo ------------------------------ TEST INIT ------------------------------",
    "test": "npm-run-all cleans lints babels uglifys unitTest docs",
    "posttest": "echo ------------------------------ TEST FINISH ------------------------------",
    "preuglifys": "echo ------------------------------ UGLIFY INIT ------------------------------",
    "uglifys": "npm-run-all uglify:server",
    "uglify:server": "uglifyjs --compress -- build/server.js",
    "postuglifys": "echo ------------------------------ UGLIFY FINISH ------------------------------",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Peterfurax",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "coveralls": "^2.11.15",
    "docco": "^0.7.0",
    "jshint": "^2.9.4",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.12.2",
    "npm-run-all": "^4.0.0",
    "nyc": "^10.0.0",
    "recluster": "^0.4.5",
    "rimraf": "^2.5.4",
    "uglify-js": "^2.7.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Peterfurax/flatplan_api.git"
  },
  "bugs": {
    "url": "https://github.com/Peterfurax/flatplan_api/issues"
  },
  "homepage": "https://github.com/Peterfurax/flatplan_api#readme"
}

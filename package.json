{
  "name": "flatplan_api",
  "version": "1.0.3",
  "description": "dev in progress",
  "main": "dist/server.js",
  "scripts": {
    "prebabel": "echo -- BABEL INIT --",
    "babel": "npm-run-all babel:*",
    "babel:src": "babel --presets es2015 src --out-dir build",
    "postbabel": "echo -- BABEL FINISH --",
    "prebuild": "echo -- BUILD INIT --",
    "build": "npm-run-all clean lint babel uglify doc --silent",
    "postbuild": "echo -- BUILD FINISH --",
    "start": "node build/server.js",
    "start:dev": "node src/server.js",
    "cluster": "node build/cluster.js",
    "cluster:dev": "node src/cluster.js",
    "preclean": "echo -- CLEAN INIT --",
    "clean": "npm-run-all clean:* ",
    "clean:coverage": "rimraf ./coverage",
    "clean:docs": "rimraf ./docs",
    "clean:build": "rimraf ./build",
    "clean:docs_Lib": "rimraf ./docs_Lib",
    "postclean": "echo -- CLEAN FINISH --",
    "prelint": "echo -- LINT INIT --",
    "lint": "npm-run-all lint:*",
    "lint:server": "jshint src/server.js",
    "lint:cluster": "jshint src/cluster.js",
    "lint:lib": "jshint src/lib/lib.js",
    "postlint": "echo -- LINT FINISH --",
    "predoc": "echo -- DOC INIT -- ",
    "doc": "npm-run-all doc:*",
    "doc:libJs": "jsdoc -c jsdoc.json",
    "doc:test": "docco test/test.js --layout linear",
    "doc:getTestErr": "docco test/provider/getTestErr.js --layout linear ",
    "doc:getTest": "docco test/provider/getTest.js --layout linear",
    "doc:server": "docco src/server.js --layout linear",
    "doc:cluster": "docco src/cluster.js --layout linear",
    "doc:lib": "docco src/lib/lib.js --layout linear",
    "postdoc": "echo -- DOC FINISH --",
    "preunitTest": "echo -- UNIT TEST INIT --",
    "unitTest": "nyc --reporter=html --reporter=text mocha",
    "postunitTest": "echo -- UNIT TEST FINISH --",
    "pretest": "echo -- TEST INIT --",
    "test": "npm-run-all build unitTest --silent",
    "posttest": "echo -- TEST FINISH --",
    "preuglify": "echo -- UGLIFY INIT --",
    "uglify": "npm-run-all uglify:*",
    "uglify:server": "uglifyjs --compress drop_console=true -nmf --output dist/server.js -- build/server.js ",
    "uglify:cluster": "uglifyjs --compress drop_console=true --output dist/cluster.js -- build/cluster.js ",
    "uglify:lib": "uglifyjs --compress drop_console=true -nmf --output dist/lib/lib.js -- build/lib/lib.js ",
    "uglify:provider": "uglifyjs --compress drop_console=true -nmf --output dist/provider/data.js -- build/provider/data.js ",
    "postuglify": "echo -- UGLIFY FINISH --",
    "prewatch": "echo -- WATCH INIT --",
    "watch": "grunt watch",
    "watch:prod": "nodenon build/server.js",
    "watch:dev": "nodenon src/server.js",
    "postwatch": "echo -- WATCH FINISH --",
    "precoverage": "echo -- COVERAGE INIT --",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postcoverage": "echo -- COVERAGE FINISH --"
  },
  "author": "Peterfurax",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "coveralls": "^2.11.15",
    "docco": "^0.7.0",
    "docdash": "^0.4.0",
    "jsdoc": "^3.4.3",
    "jshint": "^2.9.4",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.12.2",
    "npm-run-all": "^4.0.0",
    "nyc": "^10.0.0",
    "recluster": "^0.4.5",
    "rimraf": "^2.5.4",
    "uglify-js": "^2.7.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Peterfurax/flatplan_api.git"
  },
  "bugs": {
    "url": "https://github.com/Peterfurax/flatplan_api/issues"
  },
  "homepage": "https://github.com/Peterfurax/flatplan_api#readme"
}
